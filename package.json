{
  "name": "@cicerchie/svelte-swr",
  "version": "0.0.3",
  "description": "A stale-while-revalidate data fetching library for Svelte.",
  "author": "https://github.com/cicerchia",
  "license": "MIT",
  "homepage": "https://github.com/cicerchie/svelte-swr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cicerchie/svelte-swr.git"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/cicerchie"
  },
  "bugs": {
    "url": "https://github.com/cicerchie/svelte-swr/issues"
  },
  "keywords": [
    "swr",
    "svelte-swr",
    "stale-while-revalidate",
    "data-fetching",
    "data fetching"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "svelte": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "rm -rf build/* && svelte-kit build",
    "package": "rm -rf package/* && svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "postversion": "cp -r package.json .. && cd .. && npm run package"
  },
  "dependencies": {
    "@cicerchie/fsm": "0.0.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.16",
    "@sveltejs/kit": "1.0.0-next.154",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-svelte3": "3.2.0",
    "obj-str": "1.1.0",
    "prettier": "2.3.2",
    "prettier-plugin-svelte": "2.3.1",
    "svelte": "3.42.1",
    "svelte-check": "2.2.4",
    "svelte-preprocess": "4.7.4",
    "svelte2tsx": "0.4.5",
    "tslib": "2.3.1",
    "typescript": "4.3.5"
  },
  "type": "module"
}
